{
  "extends": ["next/core-web-vitals", "prettier"],
  "rules": {
    "no-console": "error",
    "no-debugger": "error",
    "prefer-const": "error",
    "no-var": "warn",
    "quotes": ["error", "double"],
    "newline-before-return": "error",
    "import/newline-after-import": ["error", { "count": 1 }]
  }
}
